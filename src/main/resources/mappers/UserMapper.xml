<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.partnerCN.dao.UserMapper">
    <insert id="add" parameterType="User">
        insert into t_user values (null,#{userName},#{password},#{name},#{sex},#{age},#{qq},#{email},#{registerTime})
    </insert>
</mapper>